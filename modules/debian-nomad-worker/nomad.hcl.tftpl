log_level = "INFO"
data_dir  = "/var/lib/nomad"
datacenter = "nak4"
bind_addr = "0.0.0.0"

advertise {
    http = "${nomad_worker_ip}"
    rpc = "${nomad_worker_ip}"
    serf = "${nomad_worker_ip}"
}

client {
  enabled = true
  servers = ["${nomad_server_ip}:4647"]
}

ports {
  http = 4646
}

consul {
  address = "${consul_master_ip}:8500"
}
